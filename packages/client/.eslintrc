{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json", // Required to have rules that rely on Types.
    "include": [
      "./src"
    ],
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "extends": [
    "plugin:@typescript-eslint/recommended", // Out of the box Typescript rules
    "standard"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "no-use-before-define": 0,
    // Uncomment this rule after it will be removed from the temoporarily listed rules below
    //    "@typescript-eslint/no-use-before-define": ["error"],
    "react/prop-types": 0,
    // temporary rules until the code is fixed. Please add permanent rules above.
    "no-var": 0,
    "no-case-declarations": 0,
    "camelcase": 0,
    "no-prototype-builtins": 0,
    "prefer-promise-reject-errors": 0,
    "comma-dangle": 0,
    "semi": 0,
    "object-curly-spacing": 0,
    "quotes": 0,
    "prefer-const": 0,
    "space-before-function-paren": 0,
    "import/no-duplicates": 0,
    "indent": 0,
    "no-multiple-empty-lines": 0,
    "no-trailing-spaces": 0,
    "key-spacing": 0,
    "eol-last": 0,
    "padded-blocks": 0,
    "space-before-blocks": 0,
    "keyword-spacing": 0,
    "brace-style": 0,
    "multiline-ternary": 0,
    "curly": 0,
    "no-multi-spaces": 0,
    "space-infix-ops": 0,
    "dot-notation": 0,
    "operator-linebreak": 0,
    "array-bracket-spacing": 0,
    "quote-props": 0,
    "object-curly-newline": 0,
    "eqeqeq": 0,
    "space-in-parens": 0,
    "no-unused-vars": 0,
    "no-undef-init": 0,
    "comma-spacing": 0,
    "no-mixed-operators": 0,
    "@typescript-eslint/no-use-before-define": 0,
    "@typescript-eslint/no-non-null-assertion": 0,
    "@typescript-eslint/no-extra-semi": 0,
    "@typescript-eslint/no-var-requires": 0,
    "@typescript-eslint/no-empty-function": 0,
    "@typescript-eslint/ban-types": 0,
    "@typescript-eslint/ban-ts-comment": 0,
    "@typescript-eslint/no-this-alias": 0,
    "@typescript-eslint/explicit-module-boundary-types": 0,
    "@typescript-eslint/no-explicit-any": 0,
    "@typescript-eslint/no-unused-vars": 0,
    "@typescript-eslint/no-inferrable-types": 0
  },
  "env": {
    "browser": true
  }
}
